{
	"$schema": "http://json.schemastore.org/template",
	"author": "RW Bradley",
	"classifications": [ "NET6.0", "C#", "WebAPI", "Windows" ],
	"tags": {
	    "language": "C#",
	    "type":"project"
	},
	"identity": "OPM.WebAPI.Template",
	"name": "OPM Web API Solution",
	"shortName": "opm-api",
	"sourceName": "Net6Template",
	"preferNameDirectory": true,
	"placeholderFilename": ".keep",
	"sources": [
		{
			"source": "./",
			"target": "./",
			"exclude": [
				"README.md",
				"**/[Bb]in/**",
				"**/[Oo]bj/**",
				".template.config/**/*",
				".vs/**/*",
				"**/*.filelist",
				"**/*.user",
				"**/*.lock.json",
				"**/.git/**",
				"*.nuspec",
				"**/node_modules/**"
			]
		}
	]
}